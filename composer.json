{
    "name": "crispcode/laravel-influx-db",
    "version": "0.0.1",
    "description": "Logging to influx db for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike",
            "email": "mike@crispcode.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Crispcode\\LaravelInfluxDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crispcode\\LaravelInfluxDB\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crispcode\\LaravelInfluxDB\\InfluxDBServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    }
}
